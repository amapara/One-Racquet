<!-- <h1> This working  minutes</h1> -->
<div class= 'parent-box'>
   <% @offer_filt_offer = Offer.first(5) %>
    <%  @directionalBallZ = 5 %>

  <% @offer_filt_offer.each do |offer| %>
   <% @directionalBallZ += 1 %>
    <div data-booking-path="<%= new_offer_booking_path(offer) %>" class = 'card img-on-top box draggable-box' style="z-index: <%= @directionalBallZ %>">
      <%= cl_image_tag offer.court.photo, id: "img_card", width: 300, height: 300 %>

      <h2> <%= offer.court.name %> </h2>

      <h2><%= offer.distance%> minutes drive </h2>

      <h2> <%= offer.match_at.strftime("%A") %>, <%= offer.match_at.strftime("%H") %>:00 </h2>

      <%= link_to new_offer_booking_path(offer) do %>
        <button type="button" class="btn btn-success">Book this offer</button>
      <% end %>
    </div>
  <% end %>
  <div class = 'box' style="z-index: 0">
    <h1><%= link_to "No suitable match? Click here to create an Offer!", new_offer_path %></h1>
  </div>
</div>


